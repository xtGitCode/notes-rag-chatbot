{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "897722688874.dkr.ecr.us-east-1.amazonaws.com/notes-rag-frontend:latest",
        "memory": 512,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8501,
            "hostPort": 80
          }
        ],
        "environment": [
          {
            "name": "BACKEND_URL",
            "value": "http://localhost:8000"
          }
        ]
      },
      {
        "name": "backend",
        "image": "897722688874.dkr.ecr.us-east-1.amazonaws.com/notes-rag-backend:latest",
        "memory": 512,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000
          }
        ],
        "environment": [
          {
            "name": "HF_TOKEN",
            "value": "#{HF_TOKEN}#"
          },
          {
            "name": "PINECONE_API_KEY",
            "value": "#{PINECONE_API_KEY}#"
          }
        ]
      }
    ]
  }